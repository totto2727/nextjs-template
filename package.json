{
  "name": "nextjs-ts-daisy",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "lint": "next lint",
    "fix": "next lint --fix && prettier --write ./src",
    "format": "prettier --check ./src",
    "prepare": "husky install",
    "component": "scaffdog generate component"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache",
      "prettier --write"
    ]
  },
  "dependencies": {
    "axios": "0.27.2",
    "daisyui": "2.19.0",
    "next": "12.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "theme-change": "2.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.3.0",
    "@types/jest": "28.1.6",
    "@types/node": "16.11.45",
    "@types/react": "18.0.15",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "autoprefixer": "10.4.7",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "7.0.4",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "scaffdog": "1.5.0",
    "tailwindcss": "3.1.6",
    "ts-jest": "28.0.7",
    "typescript": "4.7.4"
  }
}
