{
  "name": "nextjs-ts-daisy",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "lint": "next lint",
    "fix": "next lint --fix && prettier --write ./src",
    "format": "prettier --check ./src",
    "prepare": "husky install",
    "component": "scaffdog generate component"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache",
      "prettier --write"
    ]
  },
  "dependencies": {
    "axios": "0.26.1",
    "daisyui": "2.13.6",
    "next": "12.1.4",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "theme-change": "2.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.3.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.26",
    "@types/react": "18.0.1",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "autoprefixer": "10.4.4",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "postcss": "8.4.12",
    "prettier": "2.6.2",
    "scaffdog": "1.2.0",
    "tailwindcss": "3.0.23",
    "ts-jest": "27.1.4",
    "typescript": "4.6.3"
  }
}
